{
    "name": "LAWNET-Tickets",
    "version": "1.0.0",
    "description": "Discord bot created with TechDashboard",
    "main": "dist/index.js",
    "scripts": {
        "build": "npx --package=typescript tsc && mkdir -p dist/dashboard/views && cp -r src/dashboard/views/* dist/dashboard/views/",
        "start": "node dist/index.js",
        "dev": "npx -y ts-node src/index.ts",
        "deploy": "npx -y ts-node src/deploy-commands.ts"
    },
    "keywords": [
        "discord",
        "bot",
        "discord.js"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^2.0.0-dev.1766016119-427636ee7",
        "@discordjs/opus": "^0.10.0",
        "@discordjs/voice": "^0.18.0",
        "better-sqlite3": "^12.5.0",
        "connect-sqlite3": "^0.9.16",
        "cookie-parser": "^1.4.7",
        "discord.js": "^15.0.0-dev.1766016119-427636ee7",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.10",
        "express": "^5.2.1",
        "express-session": "^1.18.2",
        "ffmpeg-static": "^5.3.0",
        "google-tts-api": "^2.0.2",
        "passport": "^0.7.0",
        "passport-discord": "^0.1.4",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
